{
    "configurations": [
        {
            "name": "MacGNU",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [],
            "compilerPath": "/opt/homebrew/bin/g++-14",
            "compilerArgs": [
                "-DLOCAL",
                "-Wshadow",
                "-Wall",
                "-Wconversion",
                "-Wno-sign-conversion",
                "-Wno-unused-result",
                "-Werror=return-type"
            ],
            "cStandard": "c11",
            "cppStandard": "c++23",
            "intelliSenseMode": "gcc-x64"
        }
    ],
    "version": 4
}